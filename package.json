  "dependencies": {
    "async": "^1.5.2",
    "debug": "^2.2.0",
    "nsq-lookup": "^1.0.2",
    "nsqjs": "^0.7.12",
    "once": "^1.3.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "jscs": "^3.0.4",
    "jshint": "^2.9.2",
    "mocha": "^2.5.3",
